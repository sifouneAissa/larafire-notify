{
    "name": "sifouneaissa/larafire-notify",
    "description": "Laravel wrapper for Firebase notifications with topic and group management.",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Sifoune Aissa",
        "email": "sifouneaissa@gmail.com"
      }
    ],
    "require": {
      "php": "^8.0",
      "illuminate/support": "^8.0|^9.0|^10.0",
      "kreait/firebase-php": "^7.0"
    },
    "autoload": {
      "psr-4": {
        "Sifouneaissa\\LarafireNotify\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Sifouneaissa\\LarafireNotify\\LarafireNotifyServiceProvider"
        ]
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
  